!_TAG_FILE_FORMAT	2	/extended format; --format=1 will not append ;" to lines/
!_TAG_FILE_SORTED	1	/0=unsorted, 1=sorted, 2=foldcase/
!_TAG_PROGRAM_AUTHOR	Darren Hiebert	/dhiebert@users.sourceforge.net/
!_TAG_PROGRAM_NAME	Exuberant Ctags	//
!_TAG_PROGRAM_URL	http://ctags.sourceforge.net	/official site/
!_TAG_PROGRAM_VERSION	5.8	//
CC	makefile	/^CC = $(BIN)gcc $/;"	m
DEPS	makefile	/^DEPS = $(PRJ).dep$/;"	m
HPPS	makefile	/^HPPS :=$(wildcard *.h)$/;"	m
INCPATH	makefile	/^INCPATH = -I\/usr\/local\/include$/;"	m
LIBPATH	makefile	/^LIBPATH = -L\/usr\/local\/lib$/;"	m
LIBS	makefile	/^LIBS = -lcomctl32 -lole32 -lopengl32 -llua -liphlpapi -lgdi32$/;"	m
Line:write	新建文件夹/line.lua	/^function Line:write()$/;"	f
OBJS	makefile	/^OBJS :=$(patsubst %.c,%.o,$(SRCS))$/;"	m
PRJ	makefile	/^PRJ = ap3d$/;"	m
RRCS	makefile	/^RRCS :=$(wildcard *.rc)$/;"	m
RRCS_OBJS	makefile	/^RRCS_OBJS :=$(patsubst %.rc,%.res,$(RRCS))$/;"	m
SRCS	makefile	/^SRCS :=$(wildcard *.c)$/;"	m
WINDRES	makefile	/^WINDRES = $(BIN)windres$/;"	m
WinMain	main.c	/^int WINAPI WinMain(HINSTANCE hInstance,HINSTANCE hPrevInstance,LPSTR szCmdLine,int iCmdShow)  $/;"	f
WinProc	main.c	/^LRESULT CALLBACK WinProc(HWND hwnd,UINT message,WPARAM wParam,LPARAM lParam)  $/;"	f
on_paint	main.c	/^void on_paint(HWND hwnd)$/;"	f
on_paint	main.lua	/^function on_paint(x,hdc)$/;"	f
on_paint	新建文件夹/write_dxf.lua	/^function on_paint(x,hdc)$/;"	f
run_lua	main.c	/^void run_lua(HWND hwnd)$/;"	f
write_dxf	main.c	/^void write_dxf()$/;"	f
write_dxf	新建文件夹/write_dxf.lua	/^function write_dxf(file)$/;"	f
